#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'autorandr==1.9.post1','console_scripts','autorandr'
__requires__ = 'autorandr==1.9.post1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('autorandr==1.9.post1', 'console_scripts', 'autorandr')()
    )
